{
  "name": "babel-plugin-files",
  "version": "1.0.0",
  "description": "Babel plugin to enable importing file metadata using a glob pattern",
  "main": "index.js",
  "files": [
    "index.js",
    "index.js.map"
  ],
  "scripts": {
    "clean": "rimraf index.js index.js.map",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./ --source-maps",
    "prepublish": "npm run build",
    "test": "ava",
    "posttest": "as-i-preach",
    "watch:build": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novemberborn/babel-plugin-files.git"
  },
  "keywords": [
    "babel-plugin"
  ],
  "author": "Mark Wubben (https://novemberborn.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/novemberborn/babel-plugin-files/issues"
  },
  "homepage": "https://github.com/novemberborn/babel-plugin-files#readme",
  "devDependencies": {
    "@novemberborn/as-i-preach": "^4.0.1",
    "ava": "^0.15.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "proxyquire": "^1.7.4",
    "rimraf": "^2.5.1"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "common-path-prefix": "^1.0.0",
    "glob": "^7.0.0",
    "md5-hex": "^1.2.1",
    "mime-types": "^2.1.9",
    "pkg-dir": "^1.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
