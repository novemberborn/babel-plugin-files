{
  "name": "babel-plugin-files",
  "version": "0.0.0",
  "description": "Babel plugin to generate references to files elsewhere on the file system",
  "main": "index.js",
  "files": [
    "index.js",
    "index.js.map"
  ],
  "scripts": {
    "clean": "rimraf index.js index.js.map",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./ --source-maps",
    "prepublish": "npm run build",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novemberborn/babel-plugin-files.git"
  },
  "keywords": [
    "babel-plugin"
  ],
  "author": "Mark Wubben (https://novemberborn.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/novemberborn/babel-plugin-files/issues"
  },
  "homepage": "https://github.com/novemberborn/babel-plugin-files#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "rimraf": "^2.5.0",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "common-path-prefix": "^1.0.0",
    "glob": "^6.0.4",
    "md5-hex": "^1.2.1",
    "mime-types": "^2.1.9"
  }
}
